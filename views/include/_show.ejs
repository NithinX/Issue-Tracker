<%  for (var key in issues) { %>   
    <tr>
        <td><%= key %></td>
        <td><%= issues[key].description %></td>
        <td><%= issues[key].severity %></td>
        <td><%= issues[key].status %></td>
        <td><%= issues[key].createDate %></td>
        <td><%= issues[key].closeDate %></td>
        <td>
            <a href="/update-issue?id=<%=key%>" class="btn border-shadow update">
                <span class="text-gradient"><i class="fas fa-pencil-alt"></i></span>
            </a>
            <a href="/delete-issue?id=<%=key%>" class="btn border-shadow delete">
                <span class="text-gradient"><i class="fas fa-times"></i></span>
            </a>
        </td>
    </tr>
<% }%>
    